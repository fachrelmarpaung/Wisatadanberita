Options +FollowSymLinks
RewriteEngine On
# web

# Portal
RewriteRule ^member$ adm/login.php
RewriteRule ^member/([^/\.]+) adm/dashboard.php [L]
RewriteRule ^member/([^/\.]+)/halaman/([^/\.]+)?$ adm/dashboard.php?pages=$1&halaman=$2 [L]

RewriteRule ^wisata/([^/\.]+)?$ wisata.php?id=$1 [L]
RewriteRule ^explorewisata/([^/\.]+)?$ explorewisata.php?id=$1 [L]
RewriteRule ^berita/([^/\.]+)?$ berita.php?id=$1 [L]
RewriteRule ^exploreberita/([^/\.]+)?$ exploreberita.php?id=$1 [L]
RewriteRule ^tentang$ about.php

